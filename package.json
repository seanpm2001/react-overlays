{
  "name": "react-overlays",
  "version": "5.2.1",
  "description": "Utilities for creating robust overlay components",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-bootstrap/react-overlays.git"
  },
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "keywords": [
    "react-overlays",
    "react-component",
    "react",
    "overlay",
    "react-component",
    "tooltip",
    "bootstrap",
    "popover",
    "modal"
  ],
  "scripts": {
    "bootstrap": "yarn --network-timeout 100000 && yarn --cwd www --network-timeout 100000",
    "build": "rimraf lib && 4c build src && yarn build:popper && yarn build:pick",
    "build:pick": "cherry-pick --cwd=lib --input-dir=../src --cjs-dir=cjs --esm-dir=esm",
    "build:popper": "rollup src/popper.ts --file lib/cjs/popper.js --format cjs --name popper --plugin @rollup/plugin-node-resolve",
    "deploy-docs": "yarn --cwd www build --prefix-paths && gh-pages -d www/public",
    "lint": "eslint www/*.js www/src src test *.js  --ext .js,.ts,.tsx",
    "prepublishOnly": "yarn build",
    "release": "rollout",
    "start": "yarn --cwd www start",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "yarn lint && yarn testonly",
    "testonly": "yarn tdd --single-run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js,*.tsx": "eslint --fix --ext .js,.ts,.tsx"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "dependencies": {
    "@babel/runtime": "^7.13.8",
    "@popperjs/core": "^2.11.6",
    "@restart/hooks": "^0.4.7",
    "@types/warning": "^3.0.0",
    "dom-helpers": "^5.2.0",
    "prop-types": "^15.7.2",
    "uncontrollable": "^7.2.1",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@4c/cli": "^2.2.9",
    "@4c/rollout": "^2.2.2",
    "@4c/tsconfig": "^0.4.1",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@react-bootstrap/eslint-config": "^1.3.2",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/classnames": "^2.3.0",
    "@types/react": "^17.0.69",
    "@types/react-dom": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.10",
    "cherry-pick": "^0.5.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^7.32.0",
    "eslint-config-4catalyzer-typescript": "^3.3.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.2.3",
    "husky": "^5.2.0",
    "jquery": "^3.7.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "5.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-transition-group": "^4.4.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "simulant": "^0.2.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.9.5",
    "util": "^0.12.5",
    "webpack": "^5.27.2",
    "webpack-atoms": "^16.1.0",
    "webpack-cli": "^4.10.0"
  },
  "bugs": {
    "url": "https://github.com/react-bootstrap/react-overlays/issues"
  }
}
